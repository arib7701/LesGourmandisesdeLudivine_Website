{
  "rules": {
    "orders": {
      ".read": "auth != null",
      ".write": true
    },
    "messages": {
      ".read": "auth != null",
      ".write": true
    },
    "news": {
      "$id": {
        "likes": {
          ".write": true
        },
        "comments": {
          ".write": true
        }
      }
    },
    ".read": true,
    ".write": "auth != null",
  }
}